# Set to march value for compiler
MARCH="icelake-client"

# Set default compiler flags
CFLAGS="-march=${MARCH} -O3 -pipe -flto=thin"
CXXFLAGS="${CFLAGS}"
MAKEOPTS="-j8"
RUSTFLAGS="-C target-cpu=${MARCH}"

# Use testing branch
ACCEPT_KEYWORDS="~amd64"

# Machine-specific USE_EXPAND
BRCMFMAC_CARDS="trinidad"
VIDEO_CARDS="intel"
